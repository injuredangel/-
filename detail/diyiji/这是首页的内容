              <script>
var typlinSecondOptions = [];
var typlinThirdOptions = [];
var emptyOption = "<option value=all>不限</option>";
AM('showTips');
AM("tab",function(){
	var t=$(".structModel");
	App.tab($(".data-tab .list-tab-hd li"),{className:"current",target:t})
     $(".list-tab-hd li").first().click();
});
       AM.ready(function(){
           App.placeholder();
           App.selectBox($(".select-mod"),{fn:function(){}});


             makeHtmlFromJson()

            bindTypelin();
            bindSearchButtons()

           showSearchBox()
           
           });


       /**
        * 根据<div class=self_defined_select_place_holder>json</div> 生成
        * 自定义下拉框
        */
       function makeHtmlFromJson(){
            $("[class='self_defined_select_place_holder']").each(function(){
               var modelFieldId=$(this).attr("modelFieldId");
               var ranges=$.parseJSON($(this).html());
               var displayName=$(this).attr("displayName");
               var html=displayName+"<select id=model_field_"+modelFieldId+"><option value='all'>不限</option>";
               for(i in ranges){
                    var  dash = (ranges[i].low=="" || ranges[i].high=="") ? "" : "-"
                    var  val=  ranges[i].low+"-"+ranges[i].high;
                    if(ranges[i].low==""&&ranges[i].high!="")
                        val = "0-"+ranges[i].high;
                    if(ranges[i].low!=""&&ranges[i].high=="")
                        val = ranges[i].low+"-999999";
                    var word=ranges[i].low+dash+ranges[i].high+ranges[i].word;
                    html += "<option value="+val+">"+word+"</option>";
               }
                html += "</select>";
               $(this).parent().html(html);
           })
           $("[class='typlin_place_holder']").each(function() {
               var json = $.parseJSON($(this).html())
               var modelField = $(this).attr("modelFieldId")
               var displayName = $(this).attr("displayName");
               var html1 = ""
               var html2 = ""
               var html3 = ""
               if (json["1"] && json["1"].length > 0) {
                   html1 += "<select style='width:190px' class=typlin_1 mf=" + modelField + " id='model_field_" + modelField + "_1'>"
                   html1 += "<option value=all>不限</option>"
                   for (i in json["1"]) {
                       html1 += "<option value=" + i + ">" + json["1"][i] + "</option>";
                   }
                   html1 += "</select>";
               }
               if (json["2"] && json["2"].length > 0) {
                   typlinSecondOptions[modelField] = []
                   html2 += "<select style='width:190px' class=typlin_2 mf='" + modelField + "' id='model_field_" + modelField + "_2'>";
                   html2 += "<option value=all>不限</option>"
                   html2 += "</select>";
                   for (i in json["2"]) {
                       var options = "<option value=all>不限</option>";
                       for (j in json["2"][i]) {
                           options += "<option value=" + j + ">" + json["2"][i][j] + "</option>";
                       }
                       typlinSecondOptions[modelField][i] = options
                   }
               }
               if (json["3"] && json["3"].length > 0) {
                   typlinThirdOptions[modelField] = []
                   html3 += "<select style='width:190px' class=typlin_3 mf=" + modelField + "  id='model_field_" + modelField + "_3'>";
                   html3 += "<option value=all>不限</option>"
                   html3 += "</select>";
                   for (i in json["3"]) {
                       typlinThirdOptions[modelField][i] = []
                       for (j in json["3"][i]) {
                           var options = "<option value=all>不限</option>";
                           for (k in json["3"][i][j]) {
                               options += "<option value=" + k + ">" + json["3"][i][j][k] + "</option>";
                           }
                           typlinThirdOptions[modelField][i][j] = options
                       }
                   }
               }
               $(this).parent().html(displayName + " " + html1 + html2 + html3);
           })
       }

       /**
        * 根据 var currentParams 初始化搜索框条件
        */
        function initSearchbox(currentSearchParams){
        }
function bindTypelin() {
    $("[class=typlin_1]").each(function() {
        $(this).change(function() {
            firstChangeEvent($(this))
        })
    })
    $("[class=typlin_2]").each(function() {
        $(this).change(function() {
            secondChangeEvent($(this))
        })
    })


}
        function bindSearchButtons(){
             $("ul[id^='searchModelList']").each(function(){
                   var modelId = $(this).attr("id").split("_")[1];
                   var setting = "isSetting";
                   <!--未启用结构化优化功能-->
                   if(setting =="isNotSetting"){
                       $("#search_submit_"+modelId).click(function(){
                       var searchFlag = 0;
                       var modelId = $(this).attr("id").split("_")[2];
                       var params={};
                       params["m"]=modelId
                       params["fid"]=1996
                      $(this).parent().siblings().each(function(){
                      	var modelFieldId=$(this).attr("modelFieldId")
                        var type= $(this).attr("searchType")
                        if("S_TYPSEL S_TYPCAL".match(type)){
                        	if ($("#model_field_"+modelFieldId).val()!="all" && $("#model_field_"+modelFieldId).val()!=""){
                                 params["mf_"+modelFieldId]=$("#model_field_"+modelFieldId).val();
                                 searchFlag = 1;
                            }
                        }else if(type=="S_TYPRANGE"){
                            if ($("#model_field_"+modelFieldId+"_low").val()!="" && !isNaN($("#model_field_"+modelFieldId+"_low").val())){
                                params["mf_low_"+modelFieldId]=$("#model_field_"+modelFieldId+"_low").val();
                                searchFlag = 1;
                            }
                            if($("#model_field_"+modelFieldId+"_high").val()!="" && !isNaN($("#model_field_"+modelFieldId+"_high").val())){
                               params["mf_high_"+modelFieldId]=$("#model_field_"+modelFieldId+"_high").val();
                               searchFlag = 1;
                            }
                        }else if("S_TYPSTR".match(type)){
                            if ($("#model_field_"+modelFieldId).val()!="" && $("#model_field_"+modelFieldId).val()!="输入关键词"){
                                params["mf_"+modelFieldId]=encodeURIComponent($("#model_field_"+modelFieldId).val());
                                searchFlag = 1;
                            }
                        }else if( type=="S_TYPRANGE_SELECT"){
                            if($("#model_field_"+modelFieldId).val()!="all"){
                                 var val =$("#model_field_"+modelFieldId).val()
                                 if(val != undefined){
	                                 var low = val.split("-")[0]
	                                 var high = val.split("-")[1]
	                                if (low!=""){
	                                 params["mf_low_"+modelFieldId]=low
	                                }
	                                if(high!=""){
	                                 params["mf_high_"+modelFieldId]=high
	                                }
                                    searchFlag = 1;
                                }
                            }
                        }else if (type=="S_TYPRAD"){
                            if($('input:radio[name=model_field_'+modelFieldId+']:checked').val()){
                                params["mf_"+modelFieldId]=$('input:radio[name=model_field_'+modelFieldId+']:checked').val();
                                searchFlag = 1;
                            }
                        }else if (type=="S_PARA_SELECT"){
                            if ($("#model_field_"+modelFieldId).val()!="all"){
                                var fieldId=$("#model_field_"+modelFieldId).attr("fieldId")
                                 params["mf_"+modelFieldId]=$("#model_field_"+modelFieldId).val()
                                 params["mf_"+modelFieldId+"_field"]=fieldId;
                                 searchFlag = 1;
                            }
                        }else if (type=="S_KEYWORD"){
                            var value = $("#model_field_"+modelFieldId+"_"+modelId).val()
                            if (value!="" && value!="输入关键词"){
                                params["mf_"+modelFieldId]=encodeURIComponent(value);
                                searchFlag = 1;
                            }
                        } else if (type == "S_TYPLIN") {
                            var first = $("#model_field_" + modelFieldId + "_1").val();
                            var second = $("#model_field_" + modelFieldId + "_2").val();
                            var third = $("#model_field_" + modelFieldId + "_3").val()
                            if (first && first != "all") {
                                params["mf_" + modelFieldId + "_1"] = first;
                                searchFlag = 1;
                            }
                            if (second && second != "all") {
                                params["mf_" + modelFieldId + "_2"] = second;
                                searchFlag = 1;
                            }
                            if (third && third != "all") {
                                params["mf_" + modelFieldId + "_3"] = third;
                                searchFlag = 1;
                            }
                        }
                       })
                       if(searchFlag == 2) {
                            App.tips({message:'请适当选择一下搜索的条件',autoclose:3});
                       } else {
                            window.location.href="//ningbo.19lou.com/thread/category/structure/search/result"+packageGet(params)
                       }
                     });
                   }else{
                       <!--在配置文件中配置了的结构化贴模块-->
                       var disableModelField = new Array()

                     $("#search_submit_"+modelId).click(function(){
                       var modelId = $(this).attr("id").split("_")[2];
                       var params={};
                       params["m"]=modelId
                       paramsquery=""
                       params["fid"]=1996
                      $(this).parent().siblings().each(function(){
                      	var modelFieldId=$(this).attr("modelFieldId")
                        var type= $(this).attr("searchType")
                        if("S_TYPSEL S_TYPCAL".match(type)){
                        	if ($("#model_field_"+modelFieldId).val()!="all" && $("#model_field_"+modelFieldId).val()!=""){
                                 var flag = true;
                                 for(var i=0;i<disableModelField.length;i++){
                                      var strsrc = "mf_"+modelFieldId;
                                      var strdest = disableModelField[i];
                                      if(strsrc == strdest){
                                        flag =false;
                                      }
                                 }
                                 if(flag){
                                    if("S_TYPSEL".match(type)){
                                       paramsquery=paramsquery+" "+encodeURIComponent($("#model_field_"+modelFieldId+" option:selected").text());
                                    }

                                    if("S_TYPCAL".match(type)){
                                       paramsquery=paramsquery+" "+encodeURIComponent($("#model_field_"+modelFieldId).val());
                                    }
                                    params["mf_"+modelFieldId]=$("#model_field_"+modelFieldId).val();
                                 }else{
                                    params["mf_"+modelFieldId]=$("#model_field_"+modelFieldId).val();
                                 }
                                 //params["mf_"+modelFieldId]=$("#model_field_"+modelFieldId).val();
                            }
                        }else if(type=="S_TYPRANGE"){
                            if ($("#model_field_"+modelFieldId+"_low").val()!="" && !isNaN($("#model_field_"+modelFieldId+"_low").val())){
                                var flag = true;
                                for(var i=0;i<disableModelField.length;i++){
                                   var strsrc = "mf_low_"+modelFieldId;
                                   var strdest = disableModelField[i];
                                   if(strsrc == strdest){
                                     flag =false;
                                   }
                                }
                                if(flag){
                                    paramsquery=paramsquery+" "+$("#model_field_"+modelFieldId+"_low").val();
                                    params["mf_low_"+modelFieldId]=$("#model_field_"+modelFieldId+"_low").val();
                                }else{
                                    params["mf_low_"+modelFieldId]=$("#model_field_"+modelFieldId+"_low").val();
                                }

                            }
                            if($("#model_field_"+modelFieldId+"_high").val()!="" && !isNaN($("#model_field_"+modelFieldId+"_high").val())){
                                var flag = true;
                                for(var i=0;i<disableModelField.length;i++){
                                   var strsrc = "mf_high_"+modelFieldId;
                                   var strdest = disableModelField[i];
                                   if(strsrc == strdest){
                                     flag =false;
                                   }
                                }
                                if(flag){
                                    paramsquery=paramsquery+" "+$("#model_field_"+modelFieldId+"_high").val();
                                    params["mf_high_"+modelFieldId]=$("#model_field_"+modelFieldId+"_high").val();
                                }else{
                                    params["mf_high_"+modelFieldId]=$("#model_field_"+modelFieldId+"_high").val();
                                }

                            }
                        }else if("S_TYPSTR".match(type)){
                            if ($("#model_field_"+modelFieldId).val()!="" && $("#model_field_"+modelFieldId).val()!="输入关键词"){
                                 var flag = true;
                                 for(var i=0;i<disableModelField.length;i++){
                                      var strsrc = "mf_"+modelFieldId;
                                      var strdest = disableModelField[i];
                                      if(strsrc == strdest){
                                        flag =false;
                                      }
                                 }
                                 if(flag){
                                    paramsquery=paramsquery+" "+encodeURIComponent($("#model_field_"+modelFieldId).val());
                                    params["mf_"+modelFieldId]=encodeURIComponent($("#model_field_"+modelFieldId).val());
                                 }else{
                                    params["mf_"+modelFieldId]=encodeURIComponent($("#model_field_"+modelFieldId).val());
                                 }

                            }
                        }else if( type=="S_TYPRANGE_SELECT"){
                            if($("#model_field_"+modelFieldId).val()!="all"){
                                var val =$("#model_field_"+modelFieldId).val()
                                if(val != undefined){
	                                var low = val.split("-")[0]
	                                var high = val.split("-")[1]
	                                if (low!=""){
                                         var flag = true;
                                         for(var i=0;i<disableModelField.length;i++){
                                              var strsrc = "mf_low_"+modelFieldId;
                                              var strdest = disableModelField[i];
                                               if(strsrc == strdest){
                                                    flag =false;
                                               }
                                         }

                                         if(flag){
                                            paramsquery=paramsquery+" "+low;
                                            params["mf_low_"+modelFieldId]=low
                                         }else{
                                            params["mf_low_"+modelFieldId]=low
                                         }
	                                }

                                    if (high!=""){
                                         var flag = true;
                                         for(var i=0;i<disableModelField.length;i++){
                                              var strsrc = "mf_high_"+modelFieldId;
                                              var strdest = disableModelField[i];
                                               if(strsrc == strdest){
                                                    flag =false;
                                               }
                                         }
                                         if(flag){
                                            paramsquery=paramsquery+" "+high;
                                            params["mf_high_"+modelFieldId]=high
                                         }else{
                                            params["mf_high_"+modelFieldId]=high
                                         }
	                                }


                                   }
                            }
                        }else if (type=="S_TYPRAD"){
                            if($('input:radio[name=model_field_'+modelFieldId+']:checked').val()){
                                params["mf_"+modelFieldId]=$('input:radio[name=model_field_'+modelFieldId+']:checked').val();
                            }
                        }else if (type=="S_PARA_SELECT"){
                            if ($("#model_field_"+modelFieldId).val()!="all"){
                                var fieldId=$("#model_field_"+modelFieldId).attr("fieldId")
                                params["mf_"+modelFieldId]=$("#model_field_"+modelFieldId).val()
                                params["mf_"+modelFieldId+"_field"]=fieldId;

                            }
                        }else if (type=="S_KEYWORD"){
                            var value = $("#model_field_"+modelFieldId+"_"+modelId).val()
                            if (value!="" && value!="输入关键词"){
                                params["mf_"+modelFieldId]=encodeURIComponent(value);
                                paramsquery=paramsquery+" "+encodeURIComponent(value)

                            }
                        } else if (type == "S_TYPLIN") {
                            var first = $("#model_field_" + modelFieldId + "_1").val();
                            var second =$("#model_field_" + modelFieldId + "_2").val();
                            var third = $("#model_field_" + modelFieldId + "_3").val();
                            if (first && first != "all") {
                                var flag = true;
                                for(var i=0;i<disableModelField.length;i++){
                                   var strsrc = "mf_" + modelFieldId + "_1";
                                   var strdest = disableModelField[i];
                                   if(strsrc == strdest){
                                     flag =false;
                                   }
                                }
                                if(flag){
                                     paramsquery = paramsquery+" "+encodeURIComponent($("#model_field_" + modelFieldId + "_1"+" option:selected").text());
                                     params["mf_" + modelFieldId + "_1"] = first;
                                }else{
                                      params["mf_" + modelFieldId + "_1"] = first;
                                }

                            }
                            if (second && second != "all") {
                                var flag = true;
                                for(var i=0;i<disableModelField.length;i++){
                                   var strsrc = "mf_" + modelFieldId + "_2";
                                   var strdest = disableModelField[i];
                                   if(strsrc == strdest){
                                     flag =false;
                                   }
                                }
                                if(flag){
                                     paramsquery = paramsquery+" "+encodeURIComponent($("#model_field_" + modelFieldId + "_2"+" option:selected").text());
                                     params["mf_" + modelFieldId + "_2"] = first;
                                }else{
                                      params["mf_" + modelFieldId + "_2"] = first;
                                }

                            }
                            if (third && third != "all") {
                                 var flag = true;
                                for(var i=0;i<disableModelField.length;i++){
                                   var strsrc = "mf_" + modelFieldId + "_3";
                                   var strdest = disableModelField[i];
                                   if(strsrc == strdest){
                                     flag =false;
                                   }
                                }
                                if(flag){
                                     paramsquery = paramsquery+" "+encodeURIComponent($("#model_field_" + modelFieldId + "_3"+" option:selected").text());
                                     params["mf_" + modelFieldId + "_3"] = first;
                                }else{
                                      params["mf_" + modelFieldId + "_3"] = first;
                                }
                            }
                        }
                       })
                         if(paramsquery!=""){
                           params["query"] = paramsquery.substr(1,paramsquery.length);
                         }
                         window.location.href="//ningbo.19lou.com/thread/category/structure/search/result"+packageGet(params)
                   });
                  }
               })
        }
 /**
 * 三级联动，处理第一个下拉框的变动事件
 * @param that: jQuery obj
 */
function firstChangeEvent(that) {
    var mf = that.attr("mf");
    var $secondSelect = $("#model_field_" + mf + "_2");
    if ($secondSelect.length>0) {
        var selected = that.find("option:selected").first().attr("value");
        var html = typlinSecondOptions[mf][selected] || emptyOption
        if (html) {
            $secondSelect.attr("first", selected)
            $secondSelect.html(html)
        }
    }
    $("[class=typlin_3] option[value!='all']").remove();
}
/**
 * 三级联动，处理第二个下拉框的变动事件
 * @param that: jQuery obj
 */
function secondChangeEvent(that) {

            var mf = that.attr("mf");
            var $thirdSelect = $("#model_field_" + mf + "_3");

            if ($thirdSelect.length>0) {
               try {
                    var first = that.attr("first")
                    var selected =that.find("option:selected").first().attr("value");
                    var html = typlinThirdOptions[mf][first][selected] || emptyOption
                    if (html) {
                        $thirdSelect.html(html)
                    }
               }catch (e) {;}
            }
}
        function showSearchBox(){

                        $("#searchModelList_10060").show()
        }
        function packageGet(params){
           var s="?"
           $.each(params, function(k,v){
               s+= k+"="+v+"&"
           })
            return s.substring(0, s.length-1);
        }
